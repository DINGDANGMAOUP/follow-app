{
  "extends": ["./.eslintrc-auto-import.json","next/core-web-vitals", "next/typescript","plugin:@typescript-eslint/recommended","prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-require-imports": "off",
    "react/jsx-no-undef": "off",
    "@typescript-eslint/no-empty-object-type": "off"
  }
}
